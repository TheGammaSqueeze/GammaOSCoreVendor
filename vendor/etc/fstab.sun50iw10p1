# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
#<src>                                                 <mnt_point>  <type>   <mnt_flags and options>      <fs_mgr_flags>
system                                                 /system      ext4     ro,barrier=1                 ro,wait,first_stage_mount,logical
vendor                                                 /vendor      ext4     ro,barrier=1                 ro,wait,first_stage_mount,logical
product                                                /product     ext4     ro,barrier=1                 ro,wait,first_stage_mount,logical
system_ext                                             /system_ext  ext4     ro,barrier=1                 ro,wait,logical,first_stage_mount
odm                                                    /odm         ext4     ro,barrier=1                 ro,wait,logical,first_stage_mount
#/dev/block/by-name/UDISK                               /data        f2fs     noatime,nosuid,nodev,discard,reserve_root=32768,resgid=1065 latemount,wait,check,quota,formattable,reservedsize=128M,checkpoint=fs
/dev/block/by-name/UDISK                               /data        f2fs     noatime,nosuid,nodev,discard wait,check,formattable,quota,reservedsize=33554432
/dev/block/by-name/cache                               /cache       ext4     noatime,nosuid,nodev,barrier=1,data=ordered,nomblk_io_submit,noauto_da_alloc,errors=panic wait,check,formattable
/dev/block/by-name/metadata                            /metadata    ext4     nodev,noatime,nosuid,errors=panic wait,first_stage_mount,formattable
/dev/block/by-name/media_data                          /oem         vfat     nodev,noatime,nosuid,context=u:object_r:oemfs:s0 wait,first_stage_mount,formattable,check
/dev/block/by-name/boot                                /boot        emmc     defaults                     defaults
/dev/block/by-name/recovery                            /recovery    emmc     defaults                     defaults
/dev/block/by-name/misc                                /misc        emmc     defaults                     defaults
/dev/block/by-name/super                               /super       emmc     defaults                     defaults
/dev/block/by-name/frp                                 /persistent  emmc     defaults                     defaults
/dev/block/zram0                                       none         swap     defaults                     zramsize=75%
/devices/platform/soc/sdc0/mmc_host/mmc*               auto         auto     defaults                     voldmanaged=extsd:auto,encryptable=footer
/devices/platform/soc/usbc0/usb*                       auto         auto     defaults                     voldmanaged=usb:auto,encryptable=footer
/devices/platform/soc/5101000.ehci0-controller/usb*    auto         auto     defaults                     voldmanaged=usb:auto,encryptable=footer
/devices/platform/soc/5200000.ehci1-controller/usb*    auto         auto     defaults                     voldmanaged=usb:auto,encryptable=footer
/devices/platform/soc/5101000.ohci0-controller/usb*    auto         auto     defaults                     voldmanaged=usb:auto,encryptable=footer
/devices/platform/soc/5200000.ohci1-controller/usb*    auto         auto     defaults                     voldmanaged=usb:auto,encryptable=footer
# add for recovery
/dev/block/sda1                                        /sdcard      auto     defaults                     defaults,recoveryonly
/dev/block/sda                                         /sdcard      auto     defaults                     defaults,recoveryonly
/dev/block/mmcblk1p1                                   /sdcard      auto     defaults                     defaults,recoveryonly
/dev/block/mmcblk1                                     /sdcard      auto     defaults                     defaults,recoveryonly
/dev/block/mmcblk0p1                                   /sdcard      auto     defaults                     defaults,recoveryonly
/dev/block/mmcblk0                                     /sdcard      auto     defaults                     defaults,recoveryonly
